{
  "name": "create-epic-test",
  "version": "0.0.0",
  "description": "create epic test",
  "license": "MIT",
  "repository": "akameco/create-epic-test",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "precommit": "yarn flow && lint-staged",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "build": "babel src -d dest",
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "yarn build",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dest"
  ],
  "keywords": [
    "test",
    "redux",
    "rx",
    "observable",
    "redux-observable"
  ],
  "dependencies": {},
  "devDependencies": {
    "all-contributors-cli": "^4.11.0",
    "babel-cli": "^6.26.0",
    "babel-preset-zero": "^0.1.0",
    "eslint": "^4.18.2",
    "eslint-config-precure": "^2.6.0",
    "flow-bin": "^0.67.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "redux-observable": "^0.18.0"
  },
  "peerDependencies": {
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.6"
  }
}
